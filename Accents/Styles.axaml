<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Light">
			<SolidColorBrush x:Key="SvgColor">#2b2b2b</SolidColorBrush>
			<SolidColorBrush x:Key="FontColor">#000000</SolidColorBrush>
			<SolidColorBrush x:Key="Accent">#00adb5</SolidColorBrush>
			<SolidColorBrush x:Key="RegionColor">#eeeeee</SolidColorBrush>
			<SolidColorBrush x:Key="RegionColorDamp">#d9d9d9</SolidColorBrush>
			<Bitmap x:Key="FileOpenImage">/Assets/fileopen-black-x256.png</Bitmap>
			<Bitmap x:Key="RefreshImage">/Assets/refresh-black-x256.png</Bitmap>
			<Bitmap x:Key="ResetImage">/Assets/reset-black-x256.png</Bitmap>
		</ResourceDictionary>
		<ResourceDictionary x:Key="Dark">
			<SolidColorBrush x:Key="SvgColor">#eeeeee</SolidColorBrush>
			<SolidColorBrush x:Key="FontColor">#ffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Accent">#00adb5</SolidColorBrush>
			<SolidColorBrush x:Key="RegionColor">#2b2b2b</SolidColorBrush>
			<SolidColorBrush x:Key="RegionColorDamp">#404040</SolidColorBrush>
			<Bitmap x:Key="FileOpenImage">/Assets/fileopen-white-x256.png</Bitmap>
			<Bitmap x:Key="RefreshImage">/Assets/refresh-white-x256.png</Bitmap>
			<Bitmap x:Key="ResetImage">/Assets/reset-white-x256.png</Bitmap>
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
	
</ResourceDictionary>
